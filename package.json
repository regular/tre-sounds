{
  "name": "tre-sounds",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "browser": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regular/tre-sounds.git"
  },
  "keywords": [],
  "author": "Jan BÃ¶lsche <jan@lagomorph.de> (https://regular.codes/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/regular/tre-sounds/issues"
  },
  "homepage": "https://github.com/regular/tre-sounds#readme",
  "tre-init": {
    "plugins": {
      "tre-client": true,
      "ssb-revisions": true
    },
    "prototypes": {
      ".": true
    },
    "importers": {
      ".": true
    },
    "messages": {
      "sounds": {
        "type": "folder",
        "root": "%root",
        "branch": "%assets"
      }
    },
    "files": {
      "mp3": {
        "path": "examples/example.mp3",
        "content": {
          "branch": "%sounds",
          "root": "%root"
        }
      },
      "ogg": {
        "path": "examples/example.ogg",
        "content": {
          "branch": "%sounds",
          "root": "%root"
        }
      },
      "flac": {
        "path": "examples/example.flac",
        "content": {
          "branch": "%sounds",
          "root": "%root"
        }
      },
      "opus": {
        "path": "examples/example.opus",
        "content": {
          "branch": "%sounds",
          "root": "%root"
        }
      }
    }
  },
  "devDependencies": {
    "brfs": "^2.0.2",
    "ssb-revisions": "^1.0.1",
    "tape": "^4.9.2",
    "tre-client": "^2.1.0",
    "tre-editor-shell": "^1.5.0",
    "tre-finder": "^2.5.0",
    "tre-property-sheet": "^1.3.1",
    "tre-prototypes": "^1.6.0"
  },
  "dependencies": {
    "bl": "^3.0.0",
    "debug": "^4.1.1",
    "file-type": "^10.9.0",
    "mutant": "^3.22.1",
    "pull-stream": "^3.6.9",
    "sodium-native": "^2.2.6",
    "ssb-ref": "^2.13.9",
    "tre-activity-indicator": "^1.0.0",
    "tre-file-importer": "^3.1.5"
  }
}
